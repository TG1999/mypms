            <div class="container-fluid">

                <div class="row">
                    <!-- column -->
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                <!-- title -->
                                <div class="d-md-flex align-items-center">
                                    <div>
                                        <h4 class="card-title">Projects</h4>
                                        <h5 class="card-subtitle">Overview of all the projects</h5>
                                    </div>
                                    <div class="ml-auto">
                                        <div class="dl">
                                            <div class="text-right upgrade-btn">
                                                <button type="button" class="btn btn-info text-white" data-toggle="modal" data-target="#exampleModal" data-whatever="@getbootstrap">Add Project</button>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <!-- title -->
                                 <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel1">
                                    <div class="modal-dialog" role="document">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <h4 class="modal-title" id="exampleModalLabel1">New message</h4>
                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                                            </div>
                                            <div class="modal-body">
                                                <form method="POST" action="/addproject">
                                                    <div class="form-group">
                                                        <label for="recipient-name" class="control-label">Project Name:</label>
                                                        <input type="text" class="form-control" name='pname' id="recipient-name1">
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="recipient-name" class="control-label">Location:</label>
                                                        <input type="text" name="location" class="form-control" id="recipient-name1">
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="recipient-name" class="control-label">Manager:</label>
                                                        <input type="text" name="manager" class="form-control" id="recipient-name1">
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="recipient-name" class="control-label">Start Date:</label>
                                                        <input type="date" name="start" class="form-control" id="recipient-name1">
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="recipient-name" class="control-label">Recipient:</label>
                                                        <input type="date" name="end" class="form-control" id="recipient-name1">
                                                    </div>
                                            </div>
                                            <div class="modal-footer">
                                                <input value="Submit" type="submit" class="btn btn-primary"></input>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="modal fade" id="exampleModal1" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel1">
                                    <div class="modal-dialog" role="document">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <h4 class="modal-title" id="exampleModalLabel1">Add User</h4>
                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                                            </div>
                                            <div class="modal-body">
                                                <form method="POST" action="/adduser">
                                                    <div class="form-group">
                                                        <label for="recipient-name" class="control-label">Name:</label>
                                                        <input type="text" class="form-control" name='name'>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="recipient-name" class="control-label">Email:</label>
                                                        <input type="email" name="email" class="form-control">
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="recipient-name" class="control-label">Password:</label>
                                                        <input type="password" name="password" class="form-control">
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="recipient-name" class="control-label">Phone Number:</label>
                                                        <input type="tel" name="Phone" class="form-control">
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="recipient-name" class="control-label">User type</label>
                                                        <select name="type" class="form-control">
                                                            <option value="SiteLeader">Site Leader</option>
                                                            <option value="StoreManager">Store Manager</option>
                                                            <option value="Admin">Admin</option>
                                                        </select>
                                                    </div>

                                            </div>
                                            <div class="modal-footer">
                                                <input value="Submit" type="submit" class="btn btn-primary"></input>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="table-responsive">
                                <table class="table v-middle">
                                    <thead>
                                        <tr class="bg-light">
                                            <th class="border-top-0">Projects</th>
                                            <th class="border-top-0">Location</th>
                                            <th class="border-top-0">Manager</th>
                                            <th class="border-top-0">Start date</th>
                                            <th class="border-top-0">End Date</th>
                                            <th class="border-top-0">Tasks</th>
                                            <th class="border-top-0">BOQ</th>
                                            <th class="border-top-0">Sites</th>
                                        </tr>
                                    </thead>
                                    <tbody id="table-body-id">
                                        {{#each data}}
                                        <tr>
                                            <td>
                                                <div class="d-flex align-items-center">
                                                    <div class="m-r-10"><span class="btn btn-circle btn-info text-white" href="{{this.projectId}}">EA</span></div>
                                                    <div class="">
                                                        <h4 class="m-b-0 font-16">{{this.projectName}}</h4>
                                                    </div>
                                                </div>
                                            </td>
                                            <td>{{this.location}}</td>
                                            <td>{{this.projectManager}}</td>
                                            <td>
                                                <label class="label label-info">{{this.startDate}}</label>
                                            </td>
                                            <td><label class="label label-info">{{this.endDate}}</label></td>
                                            <td>0</td>
                                            <td>0</td>
                                            <td>0</td>
                                        </tr>
                                        {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
               
            </div>

            <footer class="footer text-center">
